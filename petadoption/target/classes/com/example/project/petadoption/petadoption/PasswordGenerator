package com.example.petadoption;

import org.springframework.security.crypto.bcrypt.BCryptPasswordEncoder;

public class PasswordGenerator {
    public static void main(String[] args) {
        BCryptPasswordEncoder encoder = new BCryptPasswordEncoder();
        System.out.println(encoder.encode("pass1234"));
        System.out.println(encoder.encode("secure5678"));
        System.out.println(encoder.encode("mypassword"));
        System.out.println(encoder.encode("pwd4567"));
        System.out.println(encoder.encode("pass7890"));
    }
}
